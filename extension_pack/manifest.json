{
  "manifest_version": 2,

  "name": "exFRIENDSion",
  "description": "A Friends themed background on every tab!  Could this be any cooler?!",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },

  "browser_action": {
    "default_icon": "icon.png"
  },

  "permissions": [
    "tabs", "http://*/*", "https://*/*", "activeTab"
  ]
}
